import String
import stdlib

class StringContainer {
  let cont: String
  fn new() => StringContainer {
    return StringContainer {
      cont: String::new()
    }
  }

  fn print_test(self) {
    println(self.cont)
  }
}

fn main() {
  let str_mem: StringContainer = StringContainer::new()
  str_mem.print_test()
}
